# This is Git's per-user configuration file.
[alias]
    lg = log --pretty=format:\"%Cgreen%h %Creset%cd %Cblue[%cn] %Creset%s%C(yellow)%d%C(reset)\" --graph --date=relative --decorate --all
    last = log -1 HEAD --stat
    s = status
    c = commit
    co = checkout
    reword = commit --amend
    uncommit = reset --soft HEAD~1
[user]
    name = Brandon Morimoto
    email = bmorimoto99@gmail.com
[color]
    status = auto
    diff = auto
    branch = auto
    interactive = auto
    grep = auto
    ui = auto
[core]
    excludesfile = /Users/brandonmorimoto/.gitignore_global
    editor = nvim
